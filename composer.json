{
    "name": "dario_swain/stash-api",
    "type": "library",
    "description": "REST-ish endpoint for interacting with Stash",
    "keywords": ["Stash API","api","sdk"],
    "license": "MIT",
    "authors": [
        {
            "name": "Ilya Pokamestov",
            "email": "dario_swain@yahoo.com",
            "homepage": "http://github.com/DarioSwain",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.4",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8",
        "satooshi/php-coveralls": "~1.0",
        "squizlabs/php_codesniffer": "~2.6",
        "friendsofphp/php-cs-fixer": "~1.12"
    },
    "autoload": {
        "psr-4": { "Swagger\\Client\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "Swagger\\Client\\" : "test/" }
    }
}
